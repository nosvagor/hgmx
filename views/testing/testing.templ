package testing

templ Main() {
	<div class="min-h-screen p-8 font-sans bg-bgc-500 text-fgc-500">
		<!-- Header Section -->
		<header class="flex items-center justify-between pb-6 mb-8 border-b border-gry-700">
			<h1 class="text-3xl font-bold text-blue-400">UI Test Suite</h1>
			<nav class="flex items-center space-x-4">
				<a href="#" class="text-fgc-300 hover:text-blue-300">Home</a>
				<a href="#" class="text-fgc-300 hover:text-blue-300">Features</a>
				<a href="#" class="px-3 py-1 text-sm bg-blue-600 rounded text-wht-500 hover:bg-blue-500">Sign Up</a>
			</nav>
		</header>
		<!-- Main Content Grid -->
		<div class="grid grid-cols-1 gap-8 lg:grid-cols-3">
			<!-- Column 1 -->
			<div class="space-y-8">
				<!-- Card Example -->
				<div class="p-6 border rounded-lg shadow-md bg-bgc-600 border-gry-600">
					<h2 class="mb-3 text-xl font-semibold text-sky-300">Standard Card</h2>
					<p class="mb-4 text-fgc-400">This is a standard card component with a title, paragraph, and a primary action button.</p>
					<button class="px-4 py-2 text-sm bg-blue-500 rounded text-wht-500 hover:bg-blue-400 focus:outline-none focus:ring-2 focus:ring-blue-400 focus:ring-opacity-50">Learn More</button>
				</div>
				<!-- Alert Examples -->
				<div class="space-y-3">
					<div class="p-3 border rounded-md bg-grn-800 border-grn-600 text-grn-100">
						<span class="font-medium">Success!</span> Your profile has been updated.
					</div>
					<div class="p-3 border rounded-md bg-yel-800 border-yel-600 text-yel-100">
						<span class="font-medium">Warning:</span> Please check your input fields.
					</div>
					<div class="p-3 text-red-100 bg-red-800 border border-red-600 rounded-md">
						<span class="font-medium">Error!</span> Could not save changes.
					</div>
					<div class="p-3 border rounded-md bg-sky-800 border-sky-600 text-sky-100">
						<span class="font-medium">Info:</span> System maintenance scheduled for tonight.
					</div>
				</div>
			</div>
			<!-- Column 2 -->
			<div class="space-y-8">
				<!-- Form Example -->
				<div class="p-6 border rounded-lg shadow-md bg-bgc-700 border-gry-700">
					<h2 class="mb-4 text-xl font-semibold text-orn-300">Contact Form</h2>
					<form class="space-y-4">
						<div>
							<label for="name" class="block mb-1 text-sm font-medium text-fgc-300">Name</label>
							<input type="text" id="name" class="w-full px-3 py-2 text-sm border rounded bg-bgc-800 border-gry-600 text-fgc-200 focus:border-blue-500 focus:ring-blue-500 focus:outline-none"/>
						</div>
						<div>
							<label for="email" class="block mb-1 text-sm font-medium text-fgc-300">Email</label>
							<input type="email" id="email" class="w-full px-3 py-2 text-sm border rounded bg-bgc-800 border-gry-600 text-fgc-200 focus:border-blue-500 focus:ring-blue-500 focus:outline-none"/>
						</div>
						<div>
							<label for="message" class="block mb-1 text-sm font-medium text-fgc-300">Message</label>
							<textarea id="message" rows="3" class="w-full px-3 py-2 text-sm border rounded bg-bgc-800 border-gry-600 text-fgc-200 focus:border-blue-500 focus:ring-blue-500 focus:outline-none"></textarea>
						</div>
						<div class="flex items-center">
							<input id="terms" type="checkbox" class="w-4 h-4 text-blue-600 rounded bg-gry-700 border-gry-500 focus:ring-blue-500"/>
							<label for="terms" class="ml-2 text-sm text-fgc-400">I agree to the terms</label>
						</div>
						<div class="flex justify-end space-x-3">
							<button type="reset" class="px-4 py-2 text-sm rounded bg-gry-600 text-fgc-200 hover:bg-gry-500">Reset</button>
							<button type="submit" class="px-4 py-2 text-sm bg-blue-600 rounded text-wht-500 hover:bg-blue-500">Submit</button>
						</div>
					</form>
				</div>
			</div>
			<!-- Column 3 -->
			<div class="space-y-8">
				<!-- Button Showcase -->
				<div class="p-6 border rounded-lg shadow-md bg-bgc-600 border-gry-600">
					<h2 class="mb-4 text-xl font-semibold text-pnk-300">Button Styles</h2>
					<div class="flex flex-wrap gap-3">
						<button class="px-4 py-2 text-sm bg-blue-500 rounded text-wht-500 hover:bg-blue-400">Primary</button>
						<button class="px-4 py-2 text-sm rounded bg-gry-600 text-fgc-200 hover:bg-gry-500">Secondary</button>
						<button class="px-4 py-2 text-sm text-blue-400 border border-blue-500 rounded hover:bg-blue-900">Outline</button>
						<button class="px-4 py-2 text-sm text-blue-400 rounded hover:bg-blue-900 hover:text-blue-300">Subtle</button>
						<button class="px-4 py-2 text-sm bg-red-600 rounded text-wht-500 hover:bg-red-500">Destructive</button>
						<button class="px-4 py-2 text-sm rounded bg-grn-900 text-bgc-500 hover:bg-grn-700">Success</button>
					</div>
				</div>
				<!-- Table Example -->
				<div class="overflow-hidden border rounded-lg shadow-md bg-bgc-700 border-gry-700">
					<h2 class="px-6 py-3 text-xl font-semibold text-lav-300">Data Table</h2>
					<table class="w-full text-sm text-left">
						<thead class="uppercase bg-bgc-800 text-fgc-300">
							<tr>
								<th scope="col" class="px-6 py-3">ID</th>
								<th scope="col" class="px-6 py-3">Name</th>
								<th scope="col" class="px-6 py-3">Status</th>
							</tr>
						</thead>
						<tbody>
							<tr class="border-b bg-bgc-700 border-gry-600 hover:bg-bgc-600">
								<td class="px-6 py-3 font-medium text-fgc-200">#001</td>
								<td class="px-6 py-3">Alice</td>
								<td class="px-6 py-3"><span class="px-2 py-0.5 text-xs rounded bg-grn-700 text-grn-100">Active</span></td>
							</tr>
							<tr class="border-b bg-bgc-750 border-gry-600 hover:bg-bgc-600">
								<!-- Slightly different bg for striping -->
								<td class="px-6 py-3 font-medium text-fgc-200">#002</td>
								<td class="px-6 py-3">Bob</td>
								<td class="px-6 py-3"><span class="px-2 py-0.5 text-xs rounded bg-yel-700 text-yel-100">Pending</span></td>
							</tr>
							<tr class="bg-bgc-700 hover:bg-bgc-600">
								<td class="px-6 py-3 font-medium text-fgc-200">#003</td>
								<td class="px-6 py-3">Charlie</td>
								<td class="px-6 py-3"><span class="px-2 py-0.5 text-xs rounded bg-red-700 text-red-100">Inactive</span></td>
							</tr>
						</tbody>
					</table>
				</div>
			</div>
		</div>
	</div>
}
